<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" stylesheets="@../../Styles/CreateUser.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.samis.biometrics.Controllers.Admin.CreateUserController">
   <children>
      <Text layoutX="462.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create New User Account">
         <font>
            <Font name="Lato Heavy Italic" size="18.0" />
         </font>
      </Text>
      <VBox layoutX="140.0" layoutY="80.0" prefHeight="500.0" prefWidth="800.0" styleClass="create_user_container">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="FirstName">
               <font>
                  <Font name="Lato Black" size="18.0" />
               </font></Text>
            <TextField fx:id="firstNameTfld" />
            <Text layoutX="10.0" layoutY="28.0" strokeType="OUTSIDE" strokeWidth="0.0" text="LastName">
               <font>
                  <Font name="Lato Black" size="18.0" />
               </font>
            </Text>
            <TextField fx:id="lastNameTfld" />
            <Text layoutX="20.0" layoutY="126.0" strokeType="OUTSIDE" strokeWidth="0.0" text="UserName">
               <font>
                  <Font name="Lato Black" size="18.0" />
               </font>
            </Text>
            <TextField fx:id="userNameTfld" layoutX="20.0" layoutY="145.0" />
            <Text layoutX="10.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
               <font>
                  <Font name="Lato Black" size="18.0" />
               </font>
            </Text>
            <TextField fx:id="passwordTfld" />
            <Text layoutX="10.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Category">
               <font>
                  <Font name="Lato Black" size="18.0" />
               </font>
            </Text>
            <TextField fx:id="categoryTfld" />
            <CheckBox mnemonicParsing="false" text="Add Student">
               <font>
                  <Font name="Lato Black" size="13.0" />
               </font></CheckBox>
            <CheckBox layoutX="20.0" layoutY="372.0" mnemonicParsing="false" text="CheckIn">
               <font>
                  <Font name="Lato Black" size="13.0" />
               </font>
            </CheckBox>
            <CheckBox layoutX="20.0" layoutY="372.0" mnemonicParsing="false" text="CheckOut">
               <font>
                  <Font name="Lato Black" size="13.0" />
               </font>
            </CheckBox>
            <Button fx:id="createUserButton" mnemonicParsing="false" styleClass="create_user_container" text="Create New User" />
         </children>
      </VBox>
   </children>
</AnchorPane>
